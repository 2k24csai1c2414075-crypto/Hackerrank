#include <stdio.h>
#include <string.h>

#define MAX 1000


int matchPattern(char G[MAX][MAX], char P[MAX][MAX], int startRow, int startCol, int R, int C, int r, int c) {
    for (int i = 0; i < r; i++) {
        
        if (strncmp(&G[startRow + i][startCol], P[i], c) != 0) {
            return 0; // pattern does not match
        }
    }
    return 1; 
}

void gridSearch(char G[MAX][MAX], char P[MAX][MAX], int R, int C, int r, int c) {
    for (int i = 0; i <= R - r; i++) {
        for (int j = 0; j <= C - c; j++) {
           
            if (strncmp(&G[i][j], P[0], c) == 0) {
                if (matchPattern(G, P, i, j, R, C, r, c)) {
                    printf("YES\n");
                    return;
                }
            }
        }
    }
    printf("NO\n");
}
